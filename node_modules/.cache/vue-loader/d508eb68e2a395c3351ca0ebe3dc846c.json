 /></router-link>\n        <router-link :to=\"{name:'about'}\"><fa-icon class=\"icon\" :icon=\"['fas', 'user-astronaut']\" /></router-link>\n        <router-link :to=\"{name:'contact'}\"><fa-icon class=\"icon\" :icon=\"['fas', 'envelope']\" /></router-link>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"Navbar\",\n  data() {\n    return {\n      currentPage: undefined\n    };\n  },\n  created() {\n    this.$router.onReady(() => {\n      this.currentPage = this.$router.currentPage;\n    });\n  },\n  methods: {\n    navigate(path) {\n      this.currentPage = this.$router.currentPage;\n\n      if (this.stacklength > 0 && path == \"home\") {\n        this.$router.go(-this.stacklength);\n        this.stacklength = 0;\n      } else if (path != \"home\") {\n        this.$router.push(path);\n        this.stacklength++;\n      }\n    }\n  }\n};\n</script>  \n\n<style>\n@media all and (orientation: portrait) {\n  .nav-bg {\n    z-index: 10;\n    position: fixed;\n    left: 0;\n    top: 0;\n    height: 60px;\n    width: 100%;\n    background-color: #2f303a;\n    padding: 0;\n    margin: 0;\n  }\n  .logo {\n    z-index: 11;\n    height: 60px;\n    width: 60px;\n    position: fixed;\n    left: 0;\n    top: -5px;\n    font-size: 3.5em;\n    color: #ff9c40;\n  }\n  .buttons {\n    z-index: 11;\n    height: 60px;\n    width: 300px;\n    position: fixed;\n    left: 40%;\n    top: 0;\n  }\n  .icon {\n    display: inline-block;\n    z-index: 11;\n    font-size: 35px;\n    position: relative;\n    color: #fff;\n    margin-right: 30px;\n    margin-top: 10px;\n  }\n}\n@media all and (orientation: landscape) {\n  .nav-bg {\n    z-index: 10;\n    position: fixed;\n    left: 0;\n    top: 0;\n    height: 100vh;\n    width: 60px;\n    background-color: #2f303a;\n    padding: 0;\n    margin: 0;\n  }\n  .logo {\n    z-index: 11;\n    height: 60px;\n    width: 60px;\n    position: fixed;\n    left: 0;\n    top: 0;\n    font-size: 3.5em;\n    color: #ff9c40;\n  }\n  .buttons {\n    z-index: 11;\n    height: 100px;\n    width: 60px;\n    position: fixed;\n    left: 0;\n    top: 30%;\n  }\n  .icon {\n    display: inline-block;\n    z-index: 11;\n    font-size: 35px;\n    position: relative;\n    color: #fff;\n    margin-top: 30px;\n    margin-right: 0;\n  }\n}\n.icon:hover {\n  transition: all 0.2s ease;\n  color: #464fa5;\n}\n.router-link-exact-active:hover {\n  color: #ff9c40;\n}\n.router-link-exact-active > .icon {\n  color: #ff9c40;\n}\n</style>"]}]}